}
.footer-section .footer-text .ft-about .fa-social a:hover {
	background: #dfa974;
	border-color: #dfa974;
}
.footer-section .footer-text .ft-contact {
	margin-bottom: 30px;
	
}
.footer-section .footer-text .ft-contact h6 {
	font-size: 14px;
